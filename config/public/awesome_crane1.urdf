<robot name="crane">
    <!-- Base link -->
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0.125" rpy="0 0 0 " /> <!-- tranform from origin to first link-->
            <geometry>
                <cylinder radius="0.5" length="0.25" />
            </geometry>
            <material
                name="">
                <color
                rgba="0.01 0.0 0.0 1" />
            </material>
        </visual>
    </link>

    <!-- Swing rotation joint -->
    <joint name="swing_joint" type="revolute">
        <parent link="base_link"/>
        <child link="column"/>
        <origin xyz="0 0 0.25" rpy="0 0 0"/> <!-- tranform from origin to first joint-->
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
    </joint>

    <!-- Column -->
    <link name="column">
        <visual>
            <origin xyz="0 0 2" rpy="0 0 0 " /><!-- tranform from first joint to second link-->
            <geometry>
                <cylinder radius="0.15" length="4.0"/>
            </geometry>
            <material
                name="">
                <color
                rgba="1.0 0.0 0.0 1" />
            </material>
        </visual>
    </link>

    <!-- Linear Lift joint -->
    <joint name="lift_joint" type="prismatic">
        <parent link="column"/>
        <child link="upper_arm"/>
        <origin xyz="0 0 0.125" rpy="0 0 0"/><!-- tranform from first joint to second joint-->
        <axis xyz="0 0 1"/>
        <limit lower="-0.1" upper="4.0" effort="100" velocity="0.05"/>
    </joint>

    <!-- upper arm link -->
    <link name="upper_arm">
        <visual>
        <origin xyz = "0.5 0 0" rpy="0 0 0 " /><!-- tranform from second joint to third link-->
            <geometry>
                <box size="1.0 0.25 0.25"/>
            </geometry>
            <material
                name="">
                <color
                rgba="0.0 1.0 0.0 1" />
            </material>
        </visual>
    </link>

    <joint name="elbow_joint" type="revolute">
        <parent link="upper_arm"/>
        <child link="lower_arm"/>
        <origin xyz="1.0 0 0" rpy="0 0 0"/><!-- tranform from second joint to third joint-->
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
    </joint>
    
    <link name="lower_arm">
        <visual>
            <origin xyz="0.5 0 -0.25" rpy="0 0 0" /><!-- tranform from third joint to fourth link-->
            <geometry>
                <box size="1.0 0.25 0.25"/>
            </geometry>
            <material
                name="">
                <color
                rgba="0.0 0.0 1.0 1" />
            </material>
        </visual>
    </link>

    <joint name="wrist_joint" type="revolute">
        <parent link="lower_arm"/>
        <child link="wrist_extension"/>
        <origin xyz="1.0 0 -0.25" rpy="0 0 0"/><!-- tranform from third joint to fourth joint-->
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="100" velocity="1.0"/>
    </joint>
    
    <link name="wrist_extension">
        <visual>
            <origin xyz="0 0 -0.625" rpy="0 0 0" /><!-- tranform from fourth joint to fifth link-->
            <geometry>
                <cylinder radius="0.25" length="1" />
            </geometry>
            <material
                name="">
                <color
                rgba="0.0 0.5 0.5 1" />
            </material>
        </visual>
    </link>
</robot>